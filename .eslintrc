
{
  "parser": "babel-eslint",
  "extends": "future/react",
  "rules": {
    "react/display-name": 0,

    "strict": [1, "never"],
    "no-var": 1,
    "no-shadow": 1,
    "no-shadow-restricted-names": 1,
    "no-unused-vars": [1, {
      "vars": "local",
      "args": "after-used"
    }],
    "no-unused-expressions": 0,
    "no-use-before-define": 1,
    "comma-dangle": [1, "never"],
    "no-cond-assign": [1, "always"],
    "no-console": 1,
    "no-debugger": 1,
    "no-alert": 1,
    "no-constant-condition": 1,
    "no-dupe-keys": 1,
    "no-duplicate-case": 1,
    "no-empty": 1,
    "no-ex-assign": 1,
    "no-extra-boolean-cast": 0,
    "no-extra-semi": 1,
    "no-func-assign": 1,
    "no-inner-declarations": 1,
    "no-invalid-regexp": 1,
    "no-irregular-whitespace": 1,
    "no-obj-calls": 1,
    "no-reserved-keys": 0,
    "no-sparse-arrays": 1,
    "no-unreachable": 1,
    "use-isnan": 1,
    "block-scoped-var": 1,
    "consistent-return": 1,
    "curly": [1, "multi-line"],
    "default-case": 1,
    "dot-notation": [1, {
      "allowKeywords": true
    }],
    "eqeqeq": 1,
    "guard-for-in": 1,
    "no-caller": 1,
    "no-else-return": 1,
    "no-eq-null": 1,
    "no-eval": 1,
    "no-extend-native": 1,
    "no-extra-bind": 1,
    "no-fallthrough": 1,
    "no-floating-decimal": 1,
    "no-implied-eval": 1,
    "no-lone-blocks": 1,
    "no-loop-func": 1,
    "no-multi-str": 1,
    "no-native-reassign": 1,
    "no-new": 1,
    "no-new-func": 1,
    "no-new-wrappers": 1,
    "no-octal": 1,
    "no-octal-escape": 1,
    "no-param-reassign": 1,
    "no-proto": 1,
    "no-redeclare": 1,
    "no-return-assign": 1,
    "no-script-url": 1,
    "no-self-compare": 1,
    "no-sequences": 1,
    "no-throw-literal": 1,
    "no-with": 1,
    "radix": 1,
    "vars-on-top": 1,
    "wrap-iife": [1, "any"],
    "yoda": 1,
    "indent": [1, 4],
    "brace-style": [1, "stroustrup", {"allowSingleLine": false}],
    "quotes": [
      1, "double", "avoid-escape"
    ],
    "camelcase": [1, {
      "properties": "never"
    }],
    "comma-spacing": [1, {
      "before": false,
      "after": true
    }],
    "comma-style": [1, "last"],
    "eol-last": 1,
    "key-spacing": [1, {
      "beforeColon": false,
      "afterColon": true
    }],
    "new-cap": [1, {
      "newIsCap": true
    }],
    "no-multiple-empty-lines": [1, {
      "max": 2
    }],
    "no-nested-ternary": 1,
    "no-new-object": 1,
    "no-spaced-func": 1,
    "no-trailing-spaces": 1,
    "no-wrap-func": 1,
    "no-underscore-dangle": 0,
    "one-var": [1, "never"],
    "padded-blocks": [1, "never"],
    "semi": [1, "always"],
    "semi-spacing": [1, {
      "before": false,
      "after": true
    }],
    "space-after-keywords": 1,
    "space-before-blocks": 1,
    "space-before-function-paren": [1, "never"],
    "space-infix-ops": 1,
    "space-return-throw-case": 1,
    "spaced-line-comment": 1
  },
  "ecmaFeatures": {
    "jsx": true,
    "modules": true,
    "blockBindings": true
  }
}
